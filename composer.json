{
    "name": "black-coffee04/ws-siasn-php",
    "description": "Software Development Kit Web Service SiASN",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "black-coffee04",
            "email": "bkdegy@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.6"
    },
    "autoload":{
        "psr-4": {
            "SiASN\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test-siasn-client": "phpunit tests/SiasnClientTest.php --colors=always",
        "test-pns": "phpunit tests/Resources/PNSTest.php --colors=always"
    }
}
